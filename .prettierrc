{
  "printWidth": 160,
  "trailingComma": "all",
  "bracketSpacing": true,
  "arrowParens": "avoid",
  "tabWidth": 2,
  "semi": true,
  "singleQuote": true,
  "importOrder": ["<THIRD_PARTY_MODULES>", "^[./]", "^components/(.*)$", "^hooks/(.*)$", "^utilities/(.*)$"],
